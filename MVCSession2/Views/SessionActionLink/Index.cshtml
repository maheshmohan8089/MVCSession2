@model IEnumerable<MVCSession2.Models.CustomerMaster>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <tr>
        <th>
          Name
        </th>
        
       
    </tr>
    @foreach (var item in Model)
    {
        <tr>
     
            <td>
                @Html.ActionLink(item.Customer_Name, "ShowDetails", new { id = item.Customer_Id }) 
       
            </td>
        </tr>
    }

</table>